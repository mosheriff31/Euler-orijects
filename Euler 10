def prime(num):
    if num == 3:
        return True
    for k in range(2,6): 
        if num % k == 0:
            stat = False
            break
    for i in range(3,num,2): 
        if num % i == 0:
            stat = False
            break
        else :
            stat = True
    return stat

sum = 2
for p in range(3,2000000,2):
    if prime(p) == True:
        sum+=p
print(sum)
