import math
def power():
    for a in range(1,1000):
        for b in range(1,1000):
            ans=(a**2)+(b**2)
            c = math.sqrt(ans)  #calculate the square root of answer(a**2 + b**2)
            add = a + b + c
            if add == 1000 :
                return (int(a * b * c))
print(power())
